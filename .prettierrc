{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "bracketSameLine": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "printWidth": 90,
  "useTabs": false,
  "endOfLine": "auto",
  "vueIndentScriptAndStyle": true,
  "plugins": ["prettier-plugin-organize-attributes", "prettier-plugin-tailwindcss"]
}
